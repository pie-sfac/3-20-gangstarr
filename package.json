{
  "name": "gangstarr-point-styler",
  "private": false,
  "version": "0.1.0",
  "description": "Point Design System Gangstarr 라이브러리 (업데이트 0.1.0 : npm minor.ver 업데이트)",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "dist/lib/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --cache --max-warnings 0",
    "format": "prettier --cache --write .",
    "preview": "vite preview",
    "copy-files": "copyfiles -u 1 src/lib/styles/**/*.css src/lib/styles/**/fonts/**/*.woff src/lib/styles/**/fonts/**/*.woff2 src/lib/assets/*.png dist/",
    "prepare": "rm -rf dist && mkdir dist && tsc && npm run copy-files"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.4",
    "swiper": "^10.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.0"
  }
}
